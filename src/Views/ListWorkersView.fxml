<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="385.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ListWorkersViewController">
   <children>
      <Label alignment="CENTER" layoutX="435.0" layoutY="8.0" prefHeight="20.0" prefWidth="155.0" stylesheets="@../css/main.css" text="Lista de Personal">
         <styleClass>
            <String fx:value="text-info" />
            <String fx:value="title" />
            <String fx:value="bold" />
         </styleClass></Label>
      <TableView fx:id="tableWorkers" focusTraversable="false" layoutX="10.0" layoutY="50.0" prefHeight="290.0" prefWidth="996.0" styleClass="panes" stylesheets="@../css/main.css">
        <columns>
          <TableColumn fx:id="colIdWorker" editable="false" prefWidth="75.0" sortable="false" style="-fx-background-color: #F7F3F5#F7F3F5;" text="ID" visible="false" />
          <TableColumn fx:id="colDocWorker" editable="false" minWidth="0.0" prefWidth="166.0" resizable="false" sortable="false" style="-fx-background-color: #F7F3F5#F7F3F5;" text="Cedula" />
            <TableColumn fx:id="colNameWorker" editable="false" minWidth="0.0" prefWidth="166.0" resizable="false" sortable="false" style="-fx-background-color: #F7F3F5#F7F3F5;" text="Nombre" />
            <TableColumn fx:id="colLastNameWorker" editable="false" minWidth="0.0" prefWidth="166.0" resizable="false" sortable="false" style="-fx-background-color: #F7F3F5#F7F3F5;" text="Apellido" />
            <TableColumn fx:id="colEmailWorker" editable="false" minWidth="0.0" prefWidth="166.0" resizable="false" sortable="false" style="-fx-background-color: #F7F3F5#F7F3F5;" text="Correo" />
            <TableColumn fx:id="colCargoWorker" editable="false" minWidth="0.0" prefWidth="166.0" resizable="false" sortable="false" style="-fx-background-color: #F7F3F5#F7F3F5;" text="Cargo" />
            <TableColumn fx:id="colPhoneWorker" editable="false" minWidth="0.0" prefWidth="166.0" resizable="false" sortable="false" style="-fx-background-color: #F7F3F5#F7F3F5;" text="Telefono" />
        </columns>
      </TableView>
      <Button fx:id="btnExportsWorkers" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#generatePdf" prefHeight="25.0" prefWidth="83.0" stylesheets="@../css/main.css" text="Exportar">
         <styleClass>
            <String fx:value="button-md" />
            <String fx:value="button-secondary" />
            <String fx:value="shadow" />
         </styleClass>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
